{
  "compilerOptions": {
    "stripInternal": false,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "newLine": "lf",
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../../dist/releases/store",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es2015",
    "lib": [
        "es2015"
    ],
    "skipLibCheck": true,
    "types": []
  },
  "files": [
    "public_api.ts"
  ],
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": false,
    "flatModuleOutFile": "store.js",
    "flatModuleId": "@ngrx-utils/store",
    "skipTemplateCodegen": true,
    "fullTemplateTypeCheck": true
  },
  "bazelOptions": {
    "suppressTsconfigOverrideWarnings": true
  }
}