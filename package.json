{
  "name": "@ngrx-utils/platform",
  "version": "0.11.0-beta.7",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cz": "git-cz",
    "format": "pretty-quick",
    "gulp": "gulp",
    "prebuildifier": "bazel build --noshow_progress :buildtools",
    "buildifier": "find . -type f \\( -name BUILD -or -name BUILD.bazel \\) ! -path \"*/node_modules/*\" | xargs $(bazel info bazel-bin)/external/com_github_bazelbuild_buildtools/buildifier/*/buildifier"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0-rc.5",
    "@angular/common": "^6.0.0-rc.5",
    "@angular/compiler": "^6.0.0-rc.5",
    "@angular/core": "^6.0.0-rc.5",
    "@angular/forms": "^6.0.0-rc.5",
    "@angular/http": "^6.0.0-rc.5",
    "@angular/platform-browser": "^6.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.5",
    "@angular/platform-server": "^6.0.0-rc.5",
    "@angular/router": "^6.0.0-rc.5",
    "@ngrx/effects": "^6.0.0-beta.1",
    "@ngrx/entity": "^6.0.0-beta.1",
    "@ngrx/router-store": "^6.0.0-beta.1",
    "@ngrx/store": "^6.0.0-beta.1",
    "@ngrx/store-devtools": "^6.0.0-beta.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.5.7",
    "@angular-devkit/build-ng-packagr": "^0.5.7",
    "@angular/bazel": "^6.0.0-rc.5",
    "@angular/cli": "^6.0.0-rc.4",
    "@angular/compiler-cli": "^6.0.0-rc.5",
    "@angular/language-service": "^6.0.0-rc.5",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "@ngrx-utils/store": "^0.11.0-beta.6",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.1",
    "@types/glob": "^5.0.35",
    "@types/gulp": "^4.0.5",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.10.8",
    "angular-quokka-plugin": "^1.0.1",
    "chalk": "^2.4.0",
    "codecov": "4.0.0-0",
    "codelyzer": "^4.2.1",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.22",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "husky": "0.15.0-rc.13",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-spec-reporter": "^0.0.32",
    "lint-staged": "^7.0.4",
    "minimist": "^1.2.0",
    "ng-packagr": "^3.0.0-rc.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "protractor": "^5.3.1",
    "puppeteer": "^1.3.0",
    "reflect-metadata": "^0.1.12",
    "resolve-bin": "^0.4.0",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.3.1",
    "tsickle": "^0.27.5",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tsutils": "^2.26.1",
    "typescript": "~2.7.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "npm run format --staged",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "quokka": {
    "plugins": "angular-quokka-plugin"
  }
}
