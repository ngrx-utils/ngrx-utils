{
  "name": "@ngrx-utils/effects",
  "version": "0.5.1",
  "description": "Utilities to help reduce your code boilerplate when working with @ngrx/effects",
  "module": "release/@ngrx-utils/effects.es5.js",
  "es2015": "release/@ngrx-utils/effects.js",
  "main": "release/bundles/effects.umd.js",
  "typings": "release/effects.d.ts",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngrx-utils/ngrx-utils.git"
  },
  "keywords": [
    "@ngrx/effects",
    "NgRx",
    "Angular",
    "Effects"
  ],
  "author": "San Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngrx-utils/ngrx-utils/issues"
  },
  "homepage": "https://github.com/ngrx-utils/ngrx-utils#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "release"
  ],
  "peerDependencies": {
    "@ngrx/store": "^5.0.0",
    "rxjs": "^5.5.6"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "spec/tsconfig.spec.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/spec/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx)"
    ],
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
